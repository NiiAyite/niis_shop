.screen_reader_only { clip: rect(1px, 1px, 1px, 1px); clip-path: polygon(0px 0px, 0px 0px, 0px 0px); height: 0px; overflow: hidden; position: absolute; width: 0px; }

.screen_reader_only_focus_visible:focus { clip: auto; clip-path: none; font-size: inherit; height: inherit; overflow: inherit; width: inherit; }

body.using-keyboard .prev:focus, body.using-keyboard .next:focus { outline-offset: -4px; }

.photo_block { background: rgb(235, 237, 243); position: relative; display: inline-block; overflow: hidden; margin: 0px 15px 30px; box-shadow: rgba(0, 0, 0, 0.05) 0px 0px 0px 1px; }

.photo_block .story { position: absolute; display: block; top: 0px; left: 0px; width: 100%; height: 100%; pointer-events: none; transition: all 0.25s ease 0s; }

.photo_block .story::before { content: ""; position: absolute; display: block; z-index: 10; background-image: url("//assets1.pxlecdn.com/assets/embed/glyph/story-62359d0320bdde8a698622271bcbce295f677ae29728a941e344b43cc6ce7d3c.png"); background-position: center center; background-repeat: no-repeat; opacity: 0.75; transition: all 0.25s ease 0s; background-size: 40%; width: 74%; height: 74%; transform: translate(-50%, -50%); top: 50%; left: 50%; }

@media (-webkit-min-device-pixel-ratio: 1.5), not all, not all, (min-resolution: 1.5dppx) {
  .photo_block .story::before { background-image: url("//assets0.pxlecdn.com/assets/embed/glyph/story@2x-49e1ea40202a3fb3505bc6c86036aab1bd9f4a4578fdc12a62d756e309f3432c.png"); background-size: 103px 103px; }
}

